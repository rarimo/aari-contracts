{
  "name": "@rarimo/aari-contracts",
  "version": "0.1.0",
  "license": "CC0-1.0",
  "author": "Rarilabs",
  "description": "",
  "keywords": [
    "solidity",
    "smart-contracts",
    "eip-7947"
  ],
  "files": [
    "**/*.sol",
    "!mock/**/*"
  ],
  "scripts": {
    "prepare": "husky",
    "compile": "npx hardhat compile",
    "coverage": "npx hardhat coverage --solcoverjs ./.solcover.ts",
    "clean": "npx hardhat clean",
    "prepare-circuits": "npx hardhat zkit verifiers",
    "test": "npm run prepare-circuits && npx hardhat test",
    "private-network": "npx hardhat node",
    "generate-types": "TYPECHAIN_FORCE=true npx hardhat typechain && npx hardhat gobind",
    "solhint-check": "solhint \"./contracts/**/*.sol\"",
    "lint-fix": "npm run lint-sol-fix && npm run lint-ts-fix && npm run lint-json-fix && npm run solhint-check",
    "lint-json-fix": "prettier --write \"./**/*.json\"",
    "lint-ts-fix": "prettier --write \"./**/*.ts\" --ignore-path .prettierignore",
    "lint-sol-fix": "prettier --write \"contracts/**/*.sol\"",
    "publish-to-npm": "npm run lint-fix && bash ./scripts/publish.sh --public"
  },
  "dependencies": {
    "@account-abstraction/contracts": "^0.8.0",
    "@openzeppelin/contracts": "5.4.0",
    "@openzeppelin/contracts-upgradeable": "5.4.0",
    "@rarimo/passport-contracts": "^0.3.1",
    "@solarity/circom-lib": "^0.2.2",
    "@solarity/solidity-lib": "3.1.4",
    "poseidon-solidity": "0.0.5"
  },
  "devDependencies": {
    "@iden3/js-crypto": "^1.3.1",
    "@nomicfoundation/ethereumjs-util": "^9.0.4",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@solarity/chai-zkit": "^0.3.3",
    "@solarity/hardhat-zkit": "^0.5.17",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.21",
    "chai": "^4.5.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.3.1",
    "ethers": "^6.14.1",
    "hardhat": "^2.24.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^2.2.3",
    "husky": "^9.1.7",
    "mocha": "^11.4.0",
    "prettier": "^3.5.0",
    "prettier-plugin-solidity": "^1.4.3",
    "solhint": "^5.1.0",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.16",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.31.1"
  }
}
